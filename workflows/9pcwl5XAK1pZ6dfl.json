    {
      "parameters": {
        "jsCode": "for (const item of $input.all()) {\n    let folder = 'General';\n    const data = item.json;\n\n    // 1. Intentar buscar si viene un objeto \"project\" o \"homeProject\" (Proyectos de n8n)\n    if (data.project && data.project.name) {\n        folder = data.project.name;\n    } else if (data.homeProject && data.homeProject.name) {\n        folder = data.homeProject.name;\n    }\n    // 2. Intentar buscar etiquetas (Fallback)\n    else if (data.tags && Array.isArray(data.tags) && data.tags.length > 0) {\n        if (data.tags[0].name) {\n            folder = data.tags[0].name;\n        } else if (typeof data.tags[0] === 'string') {\n            folder = data.tags[0];\n        }\n    }\n\n    // Limpieza de nombre de carpeta\n    folder = folder.toString().replace(/[^a-zA-Z0-9 \\-_]/g, '_');\n    item.json.folder_calculated = folder;\n}\n\nreturn $input.all();"
      },
