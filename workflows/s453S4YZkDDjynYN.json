    {
      "parameters": {
        "jsCode": "const out = [];\n\nfor (const item of items) {\n  const product = item.json.data.inventoryItem.variant.product;\n  const productType = product.productTypeForMapping;\n\n  const collections = product.collections.edges || [];\n\n  const matching = collections.filter(edge => \n    edge.node.collectionTitleForMapping === productType\n  );\n\n  out.push({\n    json: {\n      productId: product.id,\n      productTitle: product.title,\n      productType,\n      matchingCollections: matching.map(m => m.node)\n    }\n  });\n}\n\nreturn out;\n"
